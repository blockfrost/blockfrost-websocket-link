{
  "presets": ["@babel/preset-env", "@babel/preset-typescript"],
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "utils": "./src/utils",
          "methods": "./src/methods",
          "types": "./src/types"
        }
      }
    ]
  ]
}
