{
  "name": "@blockfrost/blockfrost-websocket-link",
  "version": "1.0.1",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development ts-node-dev ./src/server.ts",
    "lint": "eslint ./src/**/*.ts",
    "start": "yarn build && yarn node ./dist/src/server.js",
    "start:profiler": "yarn build && NODE_ENV=production yarn node --prof ./dist/src/server.js",
    "test-performance": "yarn ts-node-dev ./scripts/performance/index.ts",
    "test": "BLOCKFROST_PROJECT_ID='none' vitest",
    "coverage": "vitest run --coverage",
    "type-check": "tsc --project tsconfig.json"
  },
  "dependencies": {
    "@blockfrost/blockfrost-js": "4.2.0",
    "@emurgo/cardano-serialization-lib-nodejs": "^10.2.0",
    "@sentry/integrations": "^7.5.1",
    "@sentry/node": "^7.5.1",
    "@sentry/tracing": "^7.5.1",
    "bignumber.js": "9.0.2",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "got": "^11.8.3",
    "memoizee": "^0.4.15",
    "p-queue": "^6.6.2",
    "serialize-error": "^8.1.0",
    "uuid": "^8.3.2",
    "ws": "^8.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/memoizee": "^0.4.8",
    "@types/node": "^16.11.5",
    "@types/sinon": "^10.0.12",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "c8": "^7.11.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^43.0.0",
    "nock": "^13.2.8",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.7.4",
    "vite": "^2.9.14",
    "vitest": "^0.17.1"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "yarn@3.2.1"
}
