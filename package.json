{
  "name": "@blockfrost/blockfrost-websocket-link",
  "version": "1.0.1",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development ts-node-dev ./src/server.ts",
    "lint": "eslint ./src/**/*.ts",
    "start": "yarn build && yarn node ./dist/src/server.js",
    "start:profiler": "yarn build && NODE_ENV=production yarn node --prof ./dist/src/server.js",
    "test-performance": "yarn ts-node-dev ./scripts/performance/index.ts",
    "test": "PROJECT_ID='none' jest",
    "type-check": "tsc --project tsconfig.json"
  },
  "dependencies": {
    "@blockfrost/blockfrost-js": "4.2.0-beta.4",
    "@emurgo/cardano-serialization-lib-nodejs": "^10.1.0",
    "@sentry/integrations": "^6.19.7",
    "@sentry/node": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "bignumber.js": "9.0.2",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "got": "^11.8.3",
    "memoizee": "^0.4.15",
    "p-queue": "^6.6.2",
    "serialize-error": "^8.1.0",
    "uuid": "^8.3.2",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/memoizee": "^0.4.7",
    "@types/node": "^16.11.5",
    "@types/sinon": "^10.0.11",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^41.0.1",
    "jest": "^27.5.1",
    "nock": "^13.2.4",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "sinon": "^13.0.1",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.6.2"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "yarn@3.2.0"
}
