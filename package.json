{
  "name": "@blockfrost/blockfrost-websocket-link",
  "version": "2.0.0",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development ts-node-dev ./src/server.ts",
    "lint": "eslint ./src/**/*.ts",
    "start": "yarn build && yarn node ./dist/src/server.js",
    "start:profiler": "yarn build && NODE_ENV=production yarn node --prof ./dist/src/server.js",
    "test-performance": "yarn ts-node-dev ./scripts/performance/index.ts",
    "test": "BLOCKFROST_PROJECT_ID='none' vitest",
    "coverage": "vitest run --coverage",
    "type-check": "tsc --project tsconfig.json"
  },
  "dependencies": {
    "@blockfrost/blockfrost-js": "5.3.2-beta.0",
    "@emurgo/cardano-serialization-lib-nodejs": "^10.2.0",
    "@sentry/integrations": "^7.46.0",
    "@sentry/node": "^7.46.0",
    "@sentry/tracing": "^7.46.0",
    "bignumber.js": "9.1.1",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "got": "^11.8.3",
    "memoizee": "^0.4.15",
    "p-queue": "^6.6.2",
    "serialize-error": "^8.1.0",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/memoizee": "^0.4.8",
    "@types/node": "^18.0.3",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "c8": "^7.13.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "nock": "^13.3.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "sinon": "^15.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.0.3",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "yarn@3.5.0"
}
