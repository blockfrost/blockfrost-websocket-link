{
  "name": "@blockfrost/blockfrost-websocket-link",
  "version": "2.0.0",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development yarn build && yarn node ./dist/server.js",
    "lint": "eslint ./src/**/*.ts",
    "start": "yarn build && yarn node ./dist/server.js",
    "start:profiler": "yarn build && NODE_ENV=production yarn node --prof ./dist/server.js",
    "test-performance": "yarn build && yarn node ./dist/scripts/performance/index.js",
    "test": "BLOCKFROST_PROJECT_ID='none' vitest",
    "coverage": "vitest run --coverage",
    "type-check": "tsc --project tsconfig.json"
  },
  "dependencies": {
    "@blockfrost/blockfrost-js": "5.5.0",
    "@emurgo/cardano-serialization-lib-nodejs": "^11.5.0",
    "@esbuild/darwin-arm64": "npm:0.19.8",
    "@esbuild/linux-x64": "npm:0.19.8",
    "@sentry/integrations": "^7.85.0",
    "@sentry/node": "^7.85.0",
    "@sentry/tracing": "^7.85.0",
    "bignumber.js": "9.1.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "got": "^13.0.0",
    "memoizee": "^0.4.15",
    "p-queue": "^7.3.4",
    "serialize-error": "^11.0.0",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/memoizee": "^0.4.8",
    "@types/node": "^20",
    "@types/sinon": "^10.0.15",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "c8": "^7.13.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^48.0.1",
    "nock": "^13.3.2",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "sinon": "^15.2.0",
    "typescript": "5.3.3",
    "vite": "5.0.5",
    "vitest": "1.1.1"
  },
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "main": "dist/server.js",
  "packageManager": "yarn@4.0.2"
}
