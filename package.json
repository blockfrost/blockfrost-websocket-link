    "@sentry/tracing": "^6.13.3",
    "bignumber.js": "9.0.1",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "got": "^11.8.2",
    "memoizee": "^0.4.15",
    "serialize-error": "^8.1.0",
    "validator": "^13.6.0",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
